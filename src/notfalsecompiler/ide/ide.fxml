<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="notfalsecompiler.ide.IdeController">
    <stylesheets>
        <URL value="@ide.css" />
    </stylesheets>
    <children>
        <TextArea fx:id="txtBoxCode" layoutX="123.0" layoutY="32.0" prefHeight="261.0" prefWidth="356.0" promptText="Code..." styleClass="prompt-container">
            <font>
                <Font size="19.0" />
            </font>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </TextArea>
        <Button fx:id="btnCompile" layoutX="499.0" layoutY="134.0" mnemonicParsing="false" onMouseClicked="#compile" text="Compilar" />
        <Button fx:id="saveCode" layoutX="510.0" layoutY="50.0" mnemonicParsing="false" onMouseClicked="#saveFile" text="Save" />
        <Button fx:id="loadFile" layoutX="510.0" layoutY="91.0" mnemonicParsing="false" onMouseClicked="#loadFile" text="Load" />
      <TextArea fx:id="txtConsoleLog" layoutX="123.0" layoutY="309.0" prefHeight="72.0" prefWidth="355.0" style="-fx-border-color: black; -fx-border-width: 1px; -fx-border-radius: 2px; -fx-control-inner-background: #bdbdbd; -fx-opacity: 1;" />
    </children>
</AnchorPane>
